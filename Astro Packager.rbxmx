				<string name="ScriptGuid">{900EA0A2-34D7-4055-9987-3431ED8B9AC5}</string>
				<ProtectedString name="Source"><![CDATA[local plugin = plugin
local toolbar = plugin:CreateToolbar("Astro")

local importTool = toolbar:CreateButton("Astro Import","Imports Package Script","rbxassetid://9881414570")
local loader = toolbar:CreateButton("Astro Loader","Loads Packages","rbxassetid://9881415981")

type array<t> = {[number]:t} -- Array variable type
type dictionary<t> = {[string]:t} -- Dictionary variable type

local packageScript = script.Parent["Packages.Index"]

local src

local loadNewSRC = function()
	if not game:GetService("ServerScriptService"):FindFirstChild("Packages.Index") then
		src = packageScript:Clone()
		src.Parent = game:GetService("ServerScriptService")
	else
		if not game:GetService("ServerScriptService"):FindFirstChild("Packages.Index"):IsA("ModuleScript") then
			warn("[ASTRO] 'Packages.Index' must be renamed! (Not ModuleScript)")
		else
			src = game:GetService("ServerScriptService"):FindFirstChild("Packages.Index")
		end
	end
end

importTool.Click:Connect(function()
	loadNewSRC()
end)

loader.Click:Connect(function()
	if src then
		_G.Packages = require(src)
		
		loadstring(game:GetService("HttpService"):GetAsync("https://raw.githubusercontent.com/izesw/packager/main/install.lua"))(true)
		for _, s in pairs(game:GetService("ServerScriptService"):WaitForChild("Server_Packages"):GetChildren()) do
			if s:IsA("Folder") and #s:GetChildren() == 0 then
				s:Destroy()
			end
		end
		for _, s in pairs(game:GetService("ReplicatedStorage"):WaitForChild("Packages"):GetChildren()) do
			if s:IsA("Folder") and #s:GetChildren() == 0 then
				s:Destroy()
			end
		end
	else
		warn("[ASTRO] 'Packages.Index' must exist in ServerScriptService!")
	end
end)



]]></ProtectedString>
